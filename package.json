  "scripts": {
    "dev": "tsc-watch --noClear --onSuccess \"npm run start\"",
    "build": "tsc",
    "setup": "npm run build && node --experimental-specifier-resolution=node --loader ts-node/esm dist/utils/init.js",
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm dist/index.js",
    "start:pm2": "pm2 start npm --name rss-bot -- start ",
    "run": "npm run build && npm run start",
    "run:pm2": "npm run build && npm run start:pm2",
    "lint": "eslint . --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@discordjs/builders": "^1.5.0",
    "alchemy-sdk": "^2.5.0",
    "dayjs": "^1.11.7",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "ethers": "5.7.2",
    "node-json-db": "^2.1.4",
    "p-limit": "^4.0.0",
    "yaml": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
